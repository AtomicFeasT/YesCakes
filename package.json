{
  "name": "yescakes",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "gh-pages:build": "tsc && GH_PAGES=true vite build",
    "preview": "vite preview",
    "commit": "cz",
    "compile:css": "sass ./src/styles/main.scss:./public/css/main.css",
    "autoprefix:css": "postcss --use autoprefixer -b 'last 2 versions' < public/css/main.css > public/css/styles.min.css"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "glider": "^0.1.0",
    "json-loader": "^0.5.7",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "sass": "^1.52.2",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
